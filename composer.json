{
    "name": "statink/jdenticon-stat-ink",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.1",
        "php-64bit": ">= 8.1",
        "ext-date": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-spl": "*",

        "yiisoft/yii2": "~2.0.49.3"
    },
    "require-dev": {
        "jp3cki/coding-standard": "^1.0",
        "yiisoft/yii2-debug": "~2.1.25"
    },
    "replaces": {
        "bower-asset/inputmask": "*",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "*",
        "bower-asset/yii2-pjax": "*"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true
        },
        "htaccess-protect": false,
        "lock": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.1.26"
        },
        "platform-check": true,
        "process-timeout": 1800,
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
