{
    "name": "statink/jdenticon-stat-ink",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">= 8.2",
        "php-64bit": ">= 8.2",
        "ext-date": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-spl": "*",
        "jdenticon/jdenticon": "^1.0.2",
        "yiisoft/yii2": "^2.0.50"
    },
    "require-dev": {
        "deployer/deployer": "^7.4.0",
        "ergebnis/composer-normalize": "^2.43.0",
        "jp3cki/coding-standard": "^1.0",
        "proget-hq/phpstan-yii2": "^0.8.0",
        "yiisoft/yii2-debug": "^2.1.25"
    },
    "replace": {
        "bower-asset/inputmask": "*",
        "bower-asset/jquery": "*",
        "bower-asset/punycode": "*",
        "bower-asset/yii2-pjax": "*",
        "paragonie/random_compat": "*",
        "php-extended/polyfill-php80-stringable": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "yiisoft/yii2-composer": true
        },
        "htaccess-protect": false,
        "lock": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.21"
        },
        "platform-check": true,
        "process-timeout": 1800,
        "sort-packages": true
    }
}
